.text-content {
  background-color: rgb(230, 227, 227);
  padding: 25rpx;
  margin: 0 auto;
  
}

.image-group {
  
  white-space: nowrap;
  margin-top: 30px;
}

.image-group view{
  display: inline-block;
  
 
  margin-right: 10rpx;
  margin-left: 10rpx;
  
}



.btn-func {
  display: flex;
  flex-direction: column;
  width: 100%;
  position: absolute;
  bottom: 20%;
  align-items: center;
}

.btn-img {
  width: 220px;
  height: 45px;
  line-height: 45px;
  margin-bottom: 20px;
  background-color: rgb(113, 98, 250);
  color: #fff;
  border-radius: 50px;
}
.btn {
  width: 220px;
  height: 45px;
  line-height: 45px;
 background-color: rgb(113, 98, 250);
  color: #fff;
  border-radius: 50px;
 
}
.img-container{
  margin: 0 8rpx;
  position: relative;
  
  width: 160px;
  height: 120px;
  
}

.img-container image{
 width: 100%;
 height: 100%;
 border-radius: 10px;
  
}
